@model IEnumerable<Proje.Models.Tour>

@foreach (var item in Model)
{
    <div class="col-lg-4 col-sm-6">
        <div class="single_donation_item">
            <img src=@item.Icon alt="#">
            <h4>@item.name</h4>
            <p>
                @item.description
            </p>
            <a asp-action="Index" asp-controller="Tour" asp-route-id=@item.TourId class="read_btn">read more</a>
        </div>
    </div>
}

