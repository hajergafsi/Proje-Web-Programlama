<div class="col-lg-3 col-6">
    <div class="small-box @TempData["class"] ">
        <div class="inner">
            <h3>@TempData["count"]</h3>

            <p>@TempData["name"]</p>
        </div>
        <div class="icon">
            <i class="ion @TempData["icon"]"></i>
        </div>
        <a asp-controller=@TempData["name"] asp-action="Index" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
    </div>
</div>